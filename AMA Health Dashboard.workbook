ilsData on Computer\r\n| where TimeGenerated == LastHeartbeat\r\n| join HealthStateTrend on Computer\r\n| order by State, s_avg asc, TimeAgo\r\n| project   [\"Resource Id\"]=ResourceId,    \r\n            [\"_ComputerName_\"] = Computer, \r\n            [\"Computer\"]=strcat('üñ•Ô∏è ', Computer), \r\n            [\"Environment\"] = iff(ComputerEnvironment == \"Azure\", \r\n            ComputerEnvironment, Category), \r\n            [\"OS\"]=iff(isempty(OSName), OSType, OSName),\r\n            [\"Subscription\"]=SubscriptionId,\r\n            Version, \r\n            [\"Time\"]=strcat('üïí ', TimeAgo), \r\n            State,\r\n            [\"Heartbeat Trend\"]=Trend, \r\n            [\"Details\"]=Packed\r\n//| where State == '{AgentHealthSelection}' or 'ALL' == '{AgentHealthSelection}' //single selection\r\n| where todynamic('{AgentHealthSelection}') has State or \"\" == '{AgentHealthSelection}' //Multiple Selection from agent health chart",
